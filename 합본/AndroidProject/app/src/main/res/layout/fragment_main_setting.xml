<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.Main_SettingFragment"
    android:padding="10dp"
    android:background="@color/white"
    >
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <androidx.cardview.widget.CardView xmlns:tools="http://schemas.android.com/tools"
                android:id="@+id/cardview_list"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardBackgroundColor="@color/white"
                android:clickable="true"
                android:focusable="true"
                android:layout_margin="10dp"
                app:cardElevation="0dp"
                >

                <LinearLayout
                    android:id="@+id/layout_login_setting"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    >
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginBottom="15dp"
                        >
                        <FrameLayout
                            android:id="@+id/layout_icon_list"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:ignore="WebViewLayout">
                            <ImageView
                                android:id="@+id/iv_icon_setting"
                                android:layout_width="80dp"
                                android:layout_height="80dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/kakao_default_profile_image"
                                />
                            <ImageView
                                android:id="@+id/iv_icon_d_setting"
                                android:layout_width="80dp"
                                android:layout_height="80dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/kakao_default_profile_image"
                                />
                        </FrameLayout>
                    </RelativeLayout>
                    <TextView
                        android:id="@+id/tv_name_setting"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20dp"
                        android:textSize="30dp"
                        android:text="tv_name_list"
                        />
                    <LinearLayout
                        android:id="@+id/setting_AdLL"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <LinearLayout
                            android:id="@+id/ll_now_setting"
                            android:orientation="vertical"
                            android:layout_width="0px"
                            android:gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="게시중"
                                />
                            <TextView
                                android:id="@+id/tv_count_wait_setting"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/ll_wait_setting"
                            android:orientation="vertical"
                            android:layout_width="0px"
                            android:gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="대기중"
                                />
                            <TextView
                                android:id="@+id/tv_count_now_setting"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/ll_history_setting"
                            android:orientation="vertical"
                            android:layout_width="0px"
                            android:gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="과거 내역"
                                />
                            <TextView
                                android:id="@+id/tv_count_history_setting"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/ll_cancel_setting"
                            android:layout_width="0px"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="취소내역"
                                />
                            <TextView
                                android:id="@+id/tv_count_cancel_setting"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                />
                        </LinearLayout>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="10dp"
                        android:background="@color/black"
                        />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >


            </LinearLayout>
            <LinearLayout
                android:id="@+id/setting_login"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="30dp"
                >

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/ic_login"
                    android:layout_marginLeft="15dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/setting_Login"
                    android:layout_marginLeft="15dp"
                    android:textSize="20dp"
                    android:gravity="center|left"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/setting_normalSetting"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="30dp"
                >

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/ic_setting"
                    android:layout_marginLeft="15dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/normal_tittle"
                    android:layout_marginLeft="15dp"
                    android:textSize="20dp"
                    android:gravity="center|left"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/setting_clothes"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="30dp"
                >

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/ic_clothes"
                    android:layout_marginLeft="15dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/clothes_title"
                    android:layout_marginLeft="15dp"
                    android:textSize="20dp"
                    android:gravity="center|left"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/setting_ad"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:layout_marginTop="15dp"
                android:visibility="gone"
                android:layout_marginLeft="30dp"
                >

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/ic_ad"
                    android:layout_marginLeft="15dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/ad_title"
                    android:layout_marginLeft="15dp"
                    android:textSize="20dp"
                    android:gravity="center|left"
                    />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/setting_admin"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:layout_marginTop="15dp"
                android:visibility="gone"
                android:layout_marginLeft="30dp"
                >

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/ic_admin"
                    android:layout_marginLeft="15dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/admin_title"
                    android:layout_marginLeft="15dp"
                    android:textSize="20dp"
                    android:gravity="center|left"
                    />
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</ScrollView>